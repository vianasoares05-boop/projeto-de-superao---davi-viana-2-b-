  if (x_passaro + raio > cano_x and x_passaro - raio < cano_x + cano_largura):\n        if y_passaro - raio < cano_altura or y_passaro + raio > cano_altura + distancia:\n            rodando = False\n\n    # Desenho\n    tela.fill(AZUL)\n\n    # Pássaro\n    pygame.draw.circle(tela, BRANCO, (x_passaro, int(y_passaro)), raio)\n\n    # Canos\n    pygame.draw.rect(tela, VERDE, (cano_x, 0, cano_largura, cano_altura))\n    pygame.draw.rect(tela, VERDE, (cano_x, cano_altura + distancia, cano_largura, ALTURA))\n\n    # Pontos\n    texto = fonte.render(f\"Pontos: {pontos}\", True, BRANCO)\n    tela.blit(texto, (10, 10))\n\n    pygame.display.update()\n    clock.tick(60)\n\npygame.quit()\nprint(\"GAME OVER - Pontuação final:\", pontos)\n"}]}],"snapshots":[{"timestamp":1764220712380,"editorVersion":"2.0.62","text":{"main.blocks":"<xml xmlns=\"http://www.w3.org/1999/xhtml\">\n  <variables></variables>\n  <block type=\"pxt-on-start\" x=\"0\" y=\"0\"></block>\n</xml>","main.ts":" ","README.md":" ","assets.json":"","pxt.json":"{\n    \"name\": \"projeto de superação - DAVI VIANA 2 B \",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"device\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"assets.json\"\n    ],\n    \"additionalFilePaths\": []\n}\n"}}],"shares":[],"lastSaveTime":1764220963092}